{

    "manifest_version": 3,
    "name": "Time Tracker",
    "description": "Track how much time spent on ...",
    "version": "0.1",
    "permissions": ["history", "storage"],
    "background":{
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "/images/clock.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
