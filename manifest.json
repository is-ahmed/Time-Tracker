{

    "manifest_version": 2,
    "name": "Time Tracker",
    "description": "Track how much time spent on ...",
    "version": "1.0",
    "permissions": ["history", "bookmarks"],
    "background":{
      "service_worker": "background.js"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "/images/clock.png"
    }
}
